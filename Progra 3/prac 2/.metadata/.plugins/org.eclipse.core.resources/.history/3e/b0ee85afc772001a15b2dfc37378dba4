
public class Multiplos {
	public static void main(String[] args) {
		int[] multiplos5= new int[5];
		multiplos5=getmultiplos(5);
		for (int i=0;i<5;i++) {
			System.out.println(multiplos5);
		}
	}
	public static int[] getmultiplos(int n) {
		int[] arreglo=new int[n];
		for (int i=1;i<n;i++) {
			arreglo[i-1]=n*i;
		}
		return arreglo;
	}
}